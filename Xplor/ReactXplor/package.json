{
  "name": "erx-dashboard",
  "version": "1.0.0",
  "description": "ERx Dashboard",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "prod-release-clean-dist": "npm run prod-release-remove-dist && mkdir prodreleasedist",
    "prod-release-remove-dist": "rimraf ./prodreleasedist",
    "preprod-release-build": "npm run prod-release-clean-dist",
    "prod-release-build": "babel-node tools/build.js && npm run open:prod-release-dist",
    "open:prod-release-dist": "babel-node tools/distServer.js",
    "prod-release-build-package": "babel-node tools/build.js",
    "preprod-release-build-package": "npm run prod-release-clean-dist",
    "prodqa-clean-dist": "npm run prodqa-remove-dist && mkdir prodqadist",
    "prodqa-remove-dist": "rimraf ./prodqadist",
    "preprodqa-build": "npm run prodqa-clean-dist",
    "prodqa-build": "babel-node tools/prodqaBuild.js && npm run open:prodqa-dist",
    "open:prodqa-dist": "babel-node tools/prodqaDistServer.js",
    "prodqa-build-package": "babel-node tools/prodqaBuild.js",
    "preprodqa-build-package": "npm run prodqa-clean-dist",
    "devqa-clean-dist": "npm run devqa-remove-dist && mkdir devqadist",
    "devqa-remove-dist": "rimraf ./devqadist",
    "predevqa-build": "npm run devqa-clean-dist",
    "devqa-build": "babel-node tools/devqaBuild.js && npm run open:devqa-dist",
    "open:devqa-dist": "babel-node tools/devqaDistServer.js",
    "devqa-build-package": "babel-node tools/devqaBuild.js",
    "predevqa-build-package": "npm run devqa-clean-dist",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html"
  },
  "author": "venkat",
  "license": "venkat",
  "dependencies": {
    "bootstrap": "3.3.6",
    "classnames": "2.2.5",
    "es6-promise": "3.2.1",
    "globalize": "1.1.1",
    "install": "0.8.1",
    "js-base64": "2.1.9",
    "lodash": "4.16.1",
    "moment": "2.14.1",
    "multireducer": "3.0.0",
    "npm": "3.10.5",
    "react": "15.2.0",
    "react-bootstrap": "0.29.5",
    "react-bootstrap-typeahead": "0.9.2",
    "react-data-grid": "1.0.13",
    "react-date-picker": "5.3.26",
    "react-dom": "15.2.0",
    "react-prop-types": "0.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-redux": "4.0.5",
    "react-select": "1.0.0-beta14",
    "react-tab-panel": "2.2.2",
    "react-toastr": "2.7.0",
    "redux": "3.5.2",
    "redux-form": "5.3.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.13.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.2.0",
    "copy-webpack-plugin": "3.0.1",
    "coveralls": "2.11.9",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.13",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "open": "0.0.5",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.2.0",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.3",
    "sass-loader": "4.0.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-md5-hash": "0.0.5"
  }
}
